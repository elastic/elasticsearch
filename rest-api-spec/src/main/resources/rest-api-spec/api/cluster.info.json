{
  "cluster.info": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-cluster-info",
      "description": "Get cluster info"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_info/{target}",
          "methods": [
            "GET"
          ],
          "parts": {
            "target": {
              "type": "list",
              "options": [
                "_all",
                "http",
                "ingest",
                "thread_pool",
                "script"
              ],
              "description": "Limit the information returned to the specified target."
            }
          }
        }
      ]
    }
  }
}

{
  "connector.list": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/enterprise-search/current/connectors.html",
      "description": "Lists all connectors."
    },
    "stability": "experimental",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_connector",
          "methods": [
            "GET"
          ]
        }
      ]
    },
    "params": {
      "from": {
        "type": "int",
        "default": 0,
        "description": "Starting offset (default: 0)"
      },
      "size": {
        "type": "int",
        "default": 100,
        "description": "specifies a max number of results to get (default: 100)"
      }
    }
  }
}

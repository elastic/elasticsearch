{
  "connector.update_active_filtering": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-connector-update-filtering",
      "description": "Activate the connector draft filter"
    },
    "stability": "experimental",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_connector/{connector_id}/_filtering/_activate",
          "methods": [
            "PUT"
          ],
          "parts": {
            "connector_id": {
              "type": "string",
              "description": "The unique identifier of the connector to be updated."
            }
          }
        }
      ]
    }
  }
}

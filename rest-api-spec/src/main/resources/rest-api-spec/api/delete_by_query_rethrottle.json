{
  "delete_by_query_rethrottle": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-delete-by-query-rethrottle",
      "description": "Throttle a delete by query operation"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_delete_by_query/{task_id}/_rethrottle",
          "methods": [
            "POST"
          ],
          "parts": {
            "task_id": {
              "type": "string",
              "description": "The task id to rethrottle"
            }
          }
        }
      ]
    },
    "params": {
      "requests_per_second": {
        "type": "number",
        "required": true,
        "description": "The throttle to set on this request in floating sub-requests per second. -1 means set no throttle."
      }
    }
  }
}

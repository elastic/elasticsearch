{
  "delete_script": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-delete-script",
      "description": "Delete a script or search template"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_scripts/{id}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "Script ID"
            }
          }
        }
      ]
    },
    "params": {
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout"
      },
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Specify timeout for connection to master"
      }
    }
  }
}

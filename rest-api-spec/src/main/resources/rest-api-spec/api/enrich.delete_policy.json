{
  "enrich.delete_policy": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-enrich-delete-policy",
      "description": "Delete an enrich policy"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_enrich/policy/{name}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "name": {
              "type": "string",
              "description": "The name of the enrich policy"
            }
          }
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "description": "Timeout for processing on master node"
      }
    }
  }
}

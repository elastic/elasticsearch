{
  "fleet.msearch": {
    "documentation": {
      "url": null,
      "description": "Run multiple Fleet searches"
    },
    "stability": "experimental",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/x-ndjson"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_fleet/_fleet_msearch",
          "methods": [
            "GET",
            "POST"
          ]
        },
        {
          "path": "/{index}/_fleet/_fleet_msearch",
          "methods": [
            "GET",
            "POST"
          ],
          "parts": {
            "index": {
              "type": "string",
              "description": "The index name to use as the default"
            }
          }
        }
      ]
    },
    "body": {
      "description": "The request definitions (metadata-fleet search request definition pairs), separated by newlines",
      "required": true,
      "serialize": "bulk"
    }
  }
}

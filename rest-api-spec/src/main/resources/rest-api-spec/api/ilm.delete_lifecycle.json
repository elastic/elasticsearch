{
  "ilm.delete_lifecycle": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/ilm-delete-lifecycle.html",
      "description": "Delete a lifecycle policy"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ilm/policy/{policy}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "policy": {
              "type": "string",
              "description": "The name of the index lifecycle policy"
            }
          }
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout for connection to master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout"
      }
    }
  }
}

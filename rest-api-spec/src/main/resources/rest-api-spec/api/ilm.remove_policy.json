{
  "ilm.remove_policy": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ilm-remove-policy",
      "description": "Remove policies from an index"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/{index}/_ilm/remove",
          "methods": [
            "POST"
          ],
          "parts": {
            "index": {
              "type": "string",
              "description": "The name of the index to remove policy on"
            }
          }
        }
      ]
    }
  }
}

{
  "ilm.stop": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/ilm-stop.html",
      "description": "Stop the ILM plugin"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ilm/stop",
          "methods": [
            "POST"
          ]
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout for connection to master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout"
      }
    }
  }
}

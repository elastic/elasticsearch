{
  "indices.analyze": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-indices-analyze",
      "description": "Get tokens from text analysis"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_analyze",
          "methods": [
            "GET",
            "POST"
          ]
        },
        {
          "path": "/{index}/_analyze",
          "methods": [
            "GET",
            "POST"
          ],
          "parts": {
            "index": {
              "type": "string",
              "description": "The name of the index to scope the operation"
            }
          }
        }
      ]
    },
    "params": {
      "index": {
        "type": "string",
        "description": "The name of the index to scope the operation"
      }
    },
    "body": {
      "description": "Define analyzer/tokenizer parameters and the text on which the analysis should be performed",
      "required": true
    }
  }
}

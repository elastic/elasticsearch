{
  "inference.delete": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-inference-delete",
      "description": "Delete an inference endpoint"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_inference/{inference_id}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "inference_id": {
              "type": "string",
              "description": "The inference Id"
            }
          }
        },
        {
          "path": "/_inference/{task_type}/{inference_id}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "task_type": {
              "type": "enum",
              "description": "The task type",
              "options": [
                "sparse_embedding",
                "text_embedding",
                "rerank",
                "completion",
                "chat_completion"
              ]
            },
            "inference_id": {
              "type": "string",
              "description": "The inference Id"
            }
          }
        }
      ]
    },
    "params": {
      "dry_run": {
        "type": "boolean",
        "default": false,
        "description": "If true the endpoint will not be deleted and a list of ingest processors which reference this endpoint will be returned."
      },
      "force": {
        "type": "boolean",
        "default": false,
        "description": "If true the endpoint will be forcefully stopped (regardless of whether or not it is referenced by any ingest processors or semantic text fields)."
      }
    }
  }
}

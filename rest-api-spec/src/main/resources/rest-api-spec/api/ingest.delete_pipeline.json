{
  "ingest.delete_pipeline": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ingest-delete-pipeline",
      "description": "Delete pipelines"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ingest/pipeline/{id}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "Pipeline ID"
            }
          }
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout for connection to master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout"
      }
    }
  }
}

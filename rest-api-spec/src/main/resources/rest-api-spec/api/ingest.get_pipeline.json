{
  "ingest.get_pipeline": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ingest-get-pipeline",
      "description": "Get pipelines"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ingest/pipeline",
          "methods": [
            "GET"
          ]
        },
        {
          "path": "/_ingest/pipeline/{id}",
          "methods": [
            "GET"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "Comma separated list of pipeline ids. Wildcards supported"
            }
          }
        }
      ]
    },
    "params": {
      "summary": {
        "type": "boolean",
        "default": false,
        "description": "Return pipelines without their definitions (default: false)"
      },
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout for connection to master node"
      }
    }
  }
}

{
  "ingest.put_pipeline": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/ingest.html",
      "description": "Create or update a pipeline"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ingest/pipeline/{id}",
          "methods": [
            "PUT"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "Pipeline ID"
            }
          }
        }
      ]
    },
    "params": {
      "if_version": {
        "type": "int",
        "description": "Required version for optimistic concurrency control for pipeline updates"
      },
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout for connection to master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout"
      }
    },
    "body": {
      "description": "The ingest definition",
      "required": true
    }
  }
}

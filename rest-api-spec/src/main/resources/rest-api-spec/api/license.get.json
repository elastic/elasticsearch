      "description":"Get license information"
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_license",
          "methods":[
            "GET"
          ]
        }
      ]
    },
    "params":{

{
  "license.post": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/v8/operation/operation-license-post",
      "description": "Update the license"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_license",
          "methods": [
            "PUT",
            "POST"
          ]
        }
      ]
    },
    "params": {
      "acknowledge": {
        "type": "boolean",
        "default": false,
        "description": "whether the user has acknowledged acknowledge messages (default: false)"
      },
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Timeout for processing on master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Timeout for acknowledgement of update from all nodes in cluster"
      }
    },
    "body": {
      "description": "licenses to be installed",
      "required": false
    }
  }
}

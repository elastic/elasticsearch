{
  "ml.explain_data_frame_analytics": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/explain-dfanalytics.html",
      "description": "Explain data frame analytics config"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ml/data_frame/analytics/_explain",
          "methods": [
            "GET",
            "POST"
          ]
        },
        {
          "path": "/_ml/data_frame/analytics/{id}/_explain",
          "methods": [
            "GET",
            "POST"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "The ID of the data frame analytics to explain"
            }
          }
        }
      ]
    },
    "body": {
      "description": "The data frame analytics config to explain",
      "required": false
    }
  }
}

{
  "ml.get_filters": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ml-get-filters",
      "description": "Get filters"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ml/filters",
          "methods": [
            "GET"
          ]
        },
        {
          "path": "/_ml/filters/{filter_id}",
          "methods": [
            "GET"
          ],
          "parts": {
            "filter_id": {
              "type": "list",
              "description": "Comma-separated list of strings that uniquely identify a filter."
            }
          }
        }
      ]
    },
    "params": {
      "from": {
        "type": "int",
        "default": 0,
        "description": "skips a number of filters"
      },
      "size": {
        "type": "int",
        "default": 100,
        "description": "specifies a max number of filters to get"
      }
    }
  }
}

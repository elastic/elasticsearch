{
  "ml.put_calendar": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ml-put-calendar",
      "description": "Create a calendar"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ml/calendars/{calendar_id}",
          "methods": [
            "PUT"
          ],
          "parts": {
            "calendar_id": {
              "type": "string",
              "description": "The ID of the calendar to create"
            }
          }
        }
      ]
    },
    "body": {
      "description": "The calendar details",
      "required": false
    }
  }
}

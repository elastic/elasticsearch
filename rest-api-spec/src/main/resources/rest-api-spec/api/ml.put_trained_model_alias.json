{
  "ml.put_trained_model_alias": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ml-put-trained-model-alias",
      "description": "Create or update a trained model alias"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ml/trained_models/{model_id}/model_aliases/{model_alias}",
          "methods": [
            "PUT"
          ],
          "parts": {
            "model_alias": {
              "type": "string",
              "description": "The trained model alias to update"
            },
            "model_id": {
              "type": "string",
              "description": "The trained model where the model alias should be assigned"
            }
          }
        }
      ]
    },
    "params": {
      "reassign": {
        "type": "boolean",
        "default": false,
        "description": "If the model_alias already exists and points to a separate model_id, this parameter must be true. Defaults to false."
      }
    }
  }
}

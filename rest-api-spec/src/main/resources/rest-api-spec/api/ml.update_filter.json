{
  "ml.update_filter": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-ml-update-filter",
      "description": "Update a filter"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_ml/filters/{filter_id}/_update",
          "methods": [
            "POST"
          ],
          "parts": {
            "filter_id": {
              "type": "string",
              "description": "The ID of the filter to update"
            }
          }
        }
      ]
    },
    "body": {
      "description": "The filter update",
      "required": true
    }
  }
}

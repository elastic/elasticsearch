{
  "monitoring.bulk": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch",
      "description": "Send monitoring data"
    },
    "stability": "stable",
    "visibility": "private",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/x-ndjson"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_monitoring/bulk",
          "methods": [
            "POST",
            "PUT"
          ]
        }
      ]
    },
    "params": {
      "system_id": {
        "type": "string",
        "description": "Identifier of the monitored system"
      },
      "system_api_version": {
        "type": "string",
        "description": "API Version of the monitored system"
      },
      "interval": {
        "type": "string",
        "description": "Collection interval (e.g., '10s' or '10000ms') of the payload"
      }
    },
    "body": {
      "description": "The operation definition and data (action-data pairs), separated by newlines",
      "required": true,
      "serialize": "bulk"
    }
  }
}

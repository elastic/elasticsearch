{
  "query_rules.test": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-query-rules-test",
      "description": "Test a query ruleset"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_query_rules/{ruleset_id}/_test",
          "methods": [
            "POST"
          ],
          "parts": {
            "ruleset_id": {
              "type": "string",
              "description": "The unique identifier of the ruleset to test."
            }
          }
        }
      ]
    },
    "body": {
      "description": "The match criteria to test against the ruleset",
      "required": true
    }
  }
}

{
  "render_search_template": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/render-search-template-api.html",
      "description": "Render a search template"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_render/template",
          "methods": [
            "GET",
            "POST"
          ]
        },
        {
          "path": "/_render/template/{id}",
          "methods": [
            "GET",
            "POST"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "The id of the stored search template"
            }
          }
        }
      ]
    },
    "body": {
      "description": "The search definition template and its params",
      "required": true
    }
  }
}

{
  "rollup.delete_job": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-rollup-delete-job",
      "description": "Delete a rollup job"
    },
    "stability": "experimental",
    "visibility": "public",
    "deprecated": {
      "version": "8.11.0",
      "description": "Rollup APIs are deprecated, use downsampling with time series data stream instead"
    },
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_rollup/job/{id}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "The ID of the job to delete"
            }
          }
        }
      ]
    }
  }
}

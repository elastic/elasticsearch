{
  "rollup.get_rollup_caps": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/rollup-get-rollup-caps.html",
      "description": "Get the rollup job capabilities"
    },
    "stability": "experimental",
    "visibility": "public",
    "deprecated": {
      "version": "8.11.0",
      "description": "Rollup APIs are deprecated, use downsampling with time series data stream instead"
    },
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_rollup/data/{id}",
          "methods": [
            "GET"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "The ID of the index to check rollup capabilities on, or left blank for all jobs"
            }
          }
        },
        {
          "path": "/_rollup/data",
          "methods": [
            "GET"
          ]
        }
      ]
    }
  }
}

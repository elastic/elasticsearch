{
  "rollup.start_job": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/rollup-start-job.html",
      "description": "Start rollup jobs"
    },
    "stability": "experimental",
    "visibility": "public",
    "deprecated": {
      "version": "8.11.0",
      "description": "Rollup APIs are deprecated, use downsampling with time series data stream instead"
    },
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_rollup/job/{id}/_start",
          "methods": [
            "POST"
          ],
          "parts": {
            "id": {
              "type": "string",
              "description": "The ID of the job to start"
            }
          }
        }
      ]
    }
  }
}

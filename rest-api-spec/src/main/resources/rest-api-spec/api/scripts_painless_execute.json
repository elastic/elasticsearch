{
  "scripts_painless_execute": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/painless/8.19/painless-execute-api.html",
      "description": "Run a script"
    },
    "stability": "experimental",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_scripts/painless/_execute",
          "methods": [
            "GET",
            "POST"
          ]
        }
      ]
    },
    "body": {
      "description": "The script to execute",
      "required": true
    }
  }
}

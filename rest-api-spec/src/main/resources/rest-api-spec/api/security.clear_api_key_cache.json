{
  "security.clear_api_key_cache": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-security-clear-api-key-cache",
      "description": "Clear the API key cache"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_security/api_key/{ids}/_clear_cache",
          "methods": [
            "POST"
          ],
          "parts": {
            "ids": {
              "type": "list",
              "description": "A comma-separated list of IDs of API keys to clear from the cache"
            }
          }
        }
      ]
    }
  }
}

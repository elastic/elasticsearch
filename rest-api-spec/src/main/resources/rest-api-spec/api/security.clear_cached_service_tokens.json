    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_security/service/{namespace}/{service}/credential/token/{name}/_clear_cache",
          "methods":[
            "POST"
          ],
          "parts":{
            "namespace":{
              "type":"string",

{
  "security.get_role": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/security-api-get-role.html",
      "description": "Get roles"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_security/role/{name}",
          "methods": [
            "GET"
          ],
          "parts": {
            "name": {
              "type": "list",
              "description": "A comma-separated list of role names"
            }
          }
        },
        {
          "path": "/_security/role",
          "methods": [
            "GET"
          ]
        }
      ]
    }
  }
}

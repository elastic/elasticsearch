{
  "security.get_token": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-security-get-token",
      "description": "Get a token"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_security/oauth2/token",
          "methods": [
            "POST"
          ]
        }
      ]
    },
    "body": {
      "description": "The token request to get",
      "required": true
    }
  }
}

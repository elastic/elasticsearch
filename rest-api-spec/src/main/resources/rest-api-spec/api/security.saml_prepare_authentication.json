      "description": "Prepare SAML authentication"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_security/saml/prepare",
          "methods": [
            "POST"
          ]
        }
      ]
    },
    "body": {

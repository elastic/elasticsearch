{
  "security.saml_service_provider_metadata": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/security-api-saml-sp-metadata.html",
      "description": "Create SAML service provider metadata"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_security/saml/metadata/{realm_name}",
          "methods": [
            "GET"
          ],
          "parts": {
            "realm_name": {
              "type": "string",
              "description": "The name of the SAML realm to get the metadata for"
            }
          }
        }
      ]
    }
  }
}

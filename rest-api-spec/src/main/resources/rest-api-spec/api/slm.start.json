{
  "slm.start": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/slm-api-start.html",
      "description": "Start snapshot lifecycle management"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_slm/start",
          "methods": [
            "POST"
          ]
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Timeout for processing on master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Timeout for acknowledgement of update from all nodes in cluster"
      }
    }
  }
}

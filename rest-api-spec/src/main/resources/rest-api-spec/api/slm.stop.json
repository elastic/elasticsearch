{
  "slm.stop":{
    "documentation":{
      "url":"https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-slm-stop",
      "description":"Stop snapshot lifecycle management"
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_slm/stop",
          "methods":[
            "POST"
          ]
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "description": "Timeout for processing on master node"
      },
      "timeout": {
        "type": "time",
        "description": "Timeout for acknowledgement of update from all nodes in cluster"
      }
    }
  }
}

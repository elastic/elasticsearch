{
  "slm.stop":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/slm-api-stop.html",
      "description":"Turns off snapshot lifecycle management (SLM)."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_slm/stop",
          "methods":[
            "POST"
          ]
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "description": "Timeout for processing on master node"
      },
      "timeout": {
        "type": "time",
        "description": "Timeout for acknowledgement of update from all nodes in cluster"
      }
    }
  }
}

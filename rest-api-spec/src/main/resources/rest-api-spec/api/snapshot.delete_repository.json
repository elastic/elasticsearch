{
  "snapshot.delete_repository": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-snapshot-delete-repository",
      "description": "Delete snapshot repositories"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_snapshot/{repository}",
          "methods": [
            "DELETE"
          ],
          "parts": {
            "repository": {
              "type": "list",
              "description": "Name of the snapshot repository to unregister. Wildcard (`*`) patterns are supported."
            }
          }
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout for connection to master node"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Explicit operation timeout"
      }
    }
  }
}

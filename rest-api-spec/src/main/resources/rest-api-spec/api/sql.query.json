{
  "sql.query": {
    "documentation": {
      "url": "https://www.elastic.co/guide/en/elasticsearch/reference/8.19/sql-search-api.html",
      "description": "Get SQL search results"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_sql",
          "methods": [
            "POST",
            "GET"
          ]
        }
      ]
    },
    "params": {
      "format": {
        "type": "string",
        "description": "a short version of the Accept header, e.g. json, yaml"
      }
    },
    "body": {
      "description": "Use the `query` element to start a query. Use the `cursor` element to continue a query.",
      "required": true
    }
  }
}

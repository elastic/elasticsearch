      "description": "Retrieves a summary of all defined synonym sets"
    },
    "stability": "experimental",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_synonyms",
          "methods": [
            "GET"
          ]
        }
      ]
    },
    "params": {
      "from": {
        "type": "int",
        "description": "Starting offset",
        "default": 0
      },
      "size": {
        "type": "int",
        "description": "specifies a max number of results to get",
        "default": 10
      }
    }
  }
}

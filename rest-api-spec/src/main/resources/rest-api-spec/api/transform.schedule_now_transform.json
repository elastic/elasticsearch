{
  "transform.schedule_now_transform": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-transform-schedule-now-transform",
      "description": "Schedule a transform to start now"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_transform/{transform_id}/_schedule_now",
          "methods": [
            "POST"
          ],
          "parts": {
            "transform_id": {
              "type": "string",
              "required": true,
              "description": "The id of the transform."
            }
          }
        }
      ]
    },
    "params": {
      "timeout": {
        "type": "time",
        "description": "Controls the time to wait for the scheduling to take place"
      }
    }
  }
}

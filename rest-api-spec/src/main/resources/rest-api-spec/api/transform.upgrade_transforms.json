{
  "transform.upgrade_transforms": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-transform-upgrade-transforms",
      "description": "Upgrade all transforms"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_transform/_upgrade",
          "methods": [
            "POST"
          ]
        }
      ]
    },
    "params": {
      "dry_run": {
        "type": "boolean",
        "default": false,
        "description": "Whether to only check for updates but don't execute"
      },
      "timeout": {
        "type": "time",
        "default": "30s",
        "description": "Controls the time to wait for the upgrade"
      }
    }
  }
}

{
  "watcher.get_settings": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-watcher-get-settings",
      "description": "Get Watcher index settings"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_watcher/settings",
          "methods": [
            "GET"
          ]
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Specify timeout for connection to master"
      }
    }
  }
}

{
  "watcher.start": {
    "documentation": {
      "url": "https://www.elastic.co/docs/api/doc/elasticsearch/operation/operation-watcher-start",
      "description": "Start the watch service"
    },
    "stability": "stable",
    "visibility": "public",
    "headers": {
      "accept": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_watcher/_start",
          "methods": [
            "POST"
          ]
        }
      ]
    },
    "params": {
      "master_timeout": {
        "type": "time",
        "default": "30s",
        "description": "Specify timeout for connection to master"
      }
    }
  }
}

{
  "settings": {
    "auto_expand_replicas": "0-1"
  },
  "mappings": {
    "_doc" : {
      "dynamic": false,
      "_meta": {
        "version": "${fleet.version}"
      },
      "properties": {
        "coordinator_idx": {
          "type": "integer"
        },
        "data": {
          "enabled": false,
          "type": "object"
        },
        "default_fleet_server": {
          "type": "boolean"
        },
        "policy_id": {
          "type": "keyword"
        },
        "revision_idx": {
          "type": "integer"
        },
        "@timestamp": {
          "type": "date"
        },
        "unenroll_timeout": {
          "type": "integer"
        }
      }
    }
  }
}

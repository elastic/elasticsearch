{
  "index_patterns": [
    ".fleet-actions-results"
  ],
  "data_stream": {},
  "template": {
    "settings": {
      "index.lifecycle.name": ".fleet-actions-results-ilm-policy",
      "index.auto_expand_replicas": "0-1"
    },
    "mappings": {
      "_meta": {
        "version": "${fleet.version}",
        "managed_index_mappings_version": ${fleet.managed.index.version}
      },
      "dynamic": false,
      "properties": {
        "action_id": {
          "type": "keyword"
        },
        "action_input_type": {
          "type": "keyword"
        },
        "agent_id": {
          "type": "keyword"
        },
        "action_data": {
          "enabled": false,
          "type": "object"
        },
        "action_response": {
          "dynamic": true,
          "type": "object"
        },
        "data": {
          "enabled": false,
          "type": "object"
        },
        "error": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 1024
            }
          }
        },
        "@timestamp": {
          "type": "date"
        },
        "started_at": {
          "type": "date"
        },
        "completed_at": {
          "type": "date"
        }
      }
    }
  },
  "composed_of": [],
  "priority": 200,
  "version": 1
}

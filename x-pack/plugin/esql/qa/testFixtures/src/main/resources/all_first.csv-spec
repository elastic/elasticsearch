f1
required_capability: all_first

FROM all_first
| STATS 
    v1 = ALL_FIRST(x1, @timestamp),
    v2 = ALL_FIRST(x2::long, @timestamp)
;

v1:long | v2:long
null    | null
;


f2
required_capability: all_first

FROM all_first
| STATS v1 = ALL_FIRST(x1, @timestamp) BY x3
| SORT x3 ASC
;

v1:long | x3:keyword
null    | alpha
null    | bravo
2       | charlie
4       | delta
;

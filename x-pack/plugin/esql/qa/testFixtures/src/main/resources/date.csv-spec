calculateAges
FROM employees
| WHERE birth_date > NOW() - 100 years
| EVAL age = 2024 - DATE_EXTRACT("year", birth_date)
| STATS count=count(age) BY age
| SORT count DESC, age DESC
| LIMIT 5
;


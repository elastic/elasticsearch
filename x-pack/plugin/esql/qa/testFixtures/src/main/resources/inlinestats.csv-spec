required_capability: inline_stats

FROM employees
| INLINE STATS max_salary = MAX(salary), min_salary = MIN(salary) by languages
| KEEP emp_no, languages, *salary
| WHERE min_salary > 27000 or max_salary < 70000
| sort salary desc

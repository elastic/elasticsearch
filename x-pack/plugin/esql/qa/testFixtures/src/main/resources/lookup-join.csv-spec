lookupIPFromIndexStats
required_capability: join_lookup_v5

FROM sample_data
| EVAL client_ip = client_ip::keyword
| LOOKUP JOIN clientips_lookup ON client_ip
| STATS count = count(client_ip) BY env
| SORT count DESC, env ASC
;


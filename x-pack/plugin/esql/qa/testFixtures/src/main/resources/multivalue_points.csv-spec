| EVAL st_disjoint=ST_DISJOINT(locations, TO_GEOSHAPE("POLYGON ((-10 -10, -10 10, 10 10, 10 -10, -10 -10))"))
| KEEP st_intersects, st_contains_one, st_contains_none, st_contains_all, st_within_small, st_within_big, st_disjoint

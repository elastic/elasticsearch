testMvUnionAgainstAnIndex
required_capability: fn_mv_union

FROM employees
| WHERE MV_COUNT(MV_UNION(salary_change, [-7.26, 12.09])) > 2
| SORT emp_no
| LIMIT 5
| KEEP emp_no, hire_date, salary_change;

emp_no:integer | hire_date:datetime       | salary_change:double
10001          | 1986-06-26T00:00:00.000Z | [-1.36, 13.48, 14.68]
10003          | 1986-12-01T00:00:00.000Z | [1.08, 2.29, 6.97, 12.4]
10004          | 1986-12-01T00:00:00.000Z | [-1.78, -0.85, 3.6, 5.04]
10006          | 1989-06-02T00:00:00.000Z | [-6.77, -3.43, 1.63, 3.25]
10008          | 1994-09-15T00:00:00.000Z | [-5.96, -2.92, -0.98, 5.39]
;


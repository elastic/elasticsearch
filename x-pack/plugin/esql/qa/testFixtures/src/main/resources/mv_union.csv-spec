testMvUnionGeoPoint
required_capability: fn_mv_union

ROW a = ["POINT(42.97109629958868 14.7552534006536)", "POINT(23.23 14.7)"]::geo_point, b = ["POINT(42.97109629958868 14.7552534006536)", "POINT(12.12 11.22)"]::geo_point
| EVAL finalValue = MV_UNION(a, b) 
| KEEP finalValue;

finalValue:geo_point
["POINT (42.97109629958868 14.7552534006536)", "POINT (23.23 14.7)", "POINT (12.12 11.22)"]
;

testMvUnionGeoShape
required_capability: fn_mv_union

ROW a = ["POLYGON((-9 -9, -1 -9, -1 -1, -9 -1, -9 -9))","POLYGON((1 1, 9 1, 9 9, 1 9, 1 1))"], b = ["POLYGON((-5 -5, -1 -5, -1 -1, -5 -1, -5 -5))","POLYGON((1 1, 9 1, 9 9, 1 9, 1 1))"]
| EVAL finalValue = MV_UNION(TO_GEOSHAPE(a), TO_GEOSHAPE(b)) 
| KEEP finalValue;

finalValue:geo_shape
["POLYGON((-9 -9, -1 -9, -1 -1, -9 -1, -9 -9))", "POLYGON((1 1, 9 1, 9 9, 1 9, 1 1))", "POLYGON((-5 -5, -1 -5, -1 -1, -5 -1, -5 -5))"]
;


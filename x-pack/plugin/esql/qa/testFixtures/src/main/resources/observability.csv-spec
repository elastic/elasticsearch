first_long_grouped
required_capability: fn_first_last
FROM sample_data
| STATS event_duration = first_value(event_duration) BY @timestamp = BUCKET(@timestamp, 1 hour) 
| SORT @timestamp
| KEEP @timestamp, event_duration
;

@timestamp:date          | event_duration:long

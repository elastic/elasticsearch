rerank with row filtering null scores
required_capability: rerank
required_capability: fix_score_operator_planning

ROW a = ["abc", "def", "ghi"]
| MV_EXPAND a
| EVAL a = case(a == "def", null,  a)
| RERANK "foo" ON a WITH { "inference_id" : "test_reranker" }
| WHERE _score IS NOT NULL
| SORT a
;


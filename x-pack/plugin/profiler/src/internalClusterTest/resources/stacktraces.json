{
  "settings": {
    "index": {
      "number_of_shards": "16",
      "refresh_interval": "10s",
      "sort": {
        "field": [
          "Stacktrace.frame.ids"
        ]
      }
    }
  },
  "mappings": {
    "_doc": {
      "_source": {
        "mode": "synthetic"
      },
      "properties": {
        "ecs.version": {
          "type": "keyword",
          "index": true
        },
        "Stacktrace.frame.ids": {
          "type": "keyword",
          "index": false
        },
        "Stacktrace.frame.types": {
          "type": "keyword",
          "index": false
        },
        "@timestamp": {
          "type": "date",
          "format": "epoch_second"
        }
      }
    }
  }
}

{
  "ml.get_jobs": {
    "documentation": "http://www.elastic.co/guide/en/elasticsearch/reference/current/ml-get-job.html",
    "stability": "stable",
    "methods": [ "GET"],
    "url": {
      "paths": [
        "/_ml/anomaly_detectors/{job_id}",
        "/_ml/anomaly_detectors"
      ],
      "parts": {
        "job_id": {
          "type": "string",
          "description": "The ID of the jobs to fetch"
        }
      },
      "params": {
        "allow_no_jobs": {
          "type": "boolean",
          "required": false,
          "description": "Whether to ignore if a wildcard expression matches no jobs. (This includes `_all` string or when no jobs have been specified)"
        }
      }
    },
    "body": null
  }
}

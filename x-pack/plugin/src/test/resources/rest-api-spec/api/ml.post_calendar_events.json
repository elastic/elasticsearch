{
  "ml.post_calendar_events": {
    "stability": "stable",
    "methods": [ "POST" ],
    "url": {
      "paths": [ "/_ml/calendars/{calendar_id}/events" ],
      "parts": {
        "calendar_id": {
          "type": "string",
          "required": true,
          "description": "The ID of the calendar to modify"
        }
      }
    },
    "body": {
      "description" : "A list of events",
      "required" : true
    }
  }
}

{
  "watcher.activate_watch":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/watcher-api-activate-watch.html",
      "description":"Activates a currently inactive watch."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_watcher/watch/{watch_id}/_activate",
          "methods":[
            "PUT",
            "POST"
          ],
          "parts":{
            "watch_id":{
              "type":"string",
              "description":"Watch ID"
            }
          }
        }
      ]
    }
  }
}
